  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": { "foreground": "#A9DEF9" },
    "variable.constant": { "foreground": "#D19A66" },
    "variable.defaultLibrary": { "foreground": "#E06C75" }
  },
  "tokenColors": [
    {
      "scope": [ "source.ignore", "keyword.other.definition" ],
      "settings": { "foreground": "#A9DEF9" }
    },
    {
      "scope": [ "storage", "storage.type" ],
      "settings": { "foreground": "#6A76DF", "fontStyle": "italic" }
    },
    {
      "scope": [ "keyword.control" ],
      "settings": { "foreground": "#C678DD", "fontStyle": "italic" }
    },
    {
      "scope": "string",
      "settings": { "foreground": "#D19A66", "fontStyle": "italic" }
    },
    { "scope": "string.regexp", "settings": { "foreground": "#56B6C2" } },
    {
      "scope": [ "entity.name.function" ],
      "settings": { "foreground": "#61AFEF" }
    },
    {
      "scope": [ "entity.name.namespace" ],
      "settings": { "foreground": "#7CCA8D" }
    },
    {
      "scope": [ "support.function" ],
      "settings": { "foreground": "#61AFEF" }
    },
    {
      "scope": [
        "meta.class entity.name.type.class",
        "meta.class entity.name.function",
        "new.expr entity.name.function",
        "new.expr support.class"
      ],
      "settings": { "foreground": "#4CD1E0" }
    },
    {
      "scope": [
        "entity.name.type.interface",
        "entity.name.type.alias",
        "entity.name.type.enum"
      ],
      "settings": { "foreground": "#2EA7B7" }
    },
    { "scope": [ "meta.at-rule" ], "settings": { "foreground": "#9FA0FF" } },
    {
      "scope": [
        "variable.other.readwrite.alias",
        "meta.type.annotation entity.name.type",
        "meta.type.parameters entity.name.type",
        "meta.return.type entity.name.type",
        "entity.other.inherited-class",
        "meta.object.member variable.other.readwrite",
        "entity.name.type.rust"
      ],
      "settings": { "foreground": "#9FA0FF" }
    },
    {
      "scope": [ "entity.name.type.struct" ],
      "settings": { "foreground": "#E5C07B" }
    },
    {
      "scope": [ "constant.language", "support.type.builtin" ],
      "settings": { "foreground": "#6A76DF" }
    },
    { "scope": "constant.numeric", "settings": { "foreground": "#4CD1E0" } },
    {
      "scope": [
        "variable",
        "expression.ng variable.other.readwrite",
        "support.type.property-name",
        "meta.object-literal.key",
        "variable.other.rust",
        "variable.other.readwrite",
        "keyword.key"
      ],
      "settings": { "foreground": "#A9DEF9" }
    },
    {
      "scope": "punctuation.support.type.property-name",
      "settings": { "foreground": "#6A76DF" }
    },
    {
      "scope": [ "meta.ng-binding.event expression.ng variable.other.readwrite" ],
      "settings": { "foreground": "#A9DEF9" }
    },
    {
      "scope": [ "variable.other.constant", "support.variable.property" ],
      "settings": { "foreground": "#E5C07B" }
    },
    {
      "scope": "variable.other.readwrite",
      "settings": { "foreground": "#A9DEF9" }
    },
    { "scope": "variable.language", "settings": { "foreground": "#E06C75" } },
    { "scope": "variable.scss", "settings": { "foreground": "#50AF8E" } },
    {
      "scope": "variable.parameter",
      "settings": { "foreground": "#50AF8E", "fontStyle": "italic" }
    },
    {
      "scope": [ "comment", "punctuation.definition.comment" ],
      "settings": { "foreground": "#5F6A79" }
    },
    {
      "scope": [
        "source.rust meta.attribute.rust punctuation.definition.attribute.rust"
      ],
      "settings": { "foreground": "#47BFEE" }
    },
    {
      "scope": [ "source.rust meta.attribute.rust" ],
      "settings": { "foreground": "#6A76DF" }
    },
    {
      "scope": [ "keyword.operator" ],
      "settings": { "foreground": "#2EA7B7" }
    },
    {
      "scope": [ "keyword.operator.new", "keyword.operator.expression" ],
      "settings": { "foreground": "#C678DD" }
    },
    {
      "scope": [ "keyword.operator.type", "meta.brace.round", "punctuation.accessor" ],
      "settings": { "foreground": "#CACACA" }
    },
    {
      "scope": [ "keyword.other.unit" ],
      "settings": { "foreground": "#2EA7B7" }
    },
    {
      "scope": [ "meta.brace.round", "punctuation.accessor" ],
      "settings": { "foreground": "#ABABAB" }
    },
    {
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded.rust"
      ],
      "settings": { "foreground": "#C678DD" }
    },
    {
      "scope": [ "punctuation.section.embedded.rust" ],
      "settings": { "foreground": "#E5C07B" }
    },
    {
      "scope": [ "support.type.primitive" ],
      "settings": { "foreground": "#98C379" }
    },
    {
      "scope": [ "support.class.builtin", "meta.block entity.name.type" ],
      "settings": { "foreground": "#E06C75" }
    },
    {
      "scope": [ "meta.class support.class.error" ],
      "settings": { "foreground": "#E06C75" }
    },
    { "scope": "entity.name.tag", "settings": { "foreground": "#E06C75" } },
    {
      "scope": "entity.name.tag support.class.component",
      "settings": { "foreground": "#FF616E" }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "punctuation.definition.ng-binding-name"
      ],
      "settings": { "foreground": "#98C379", "fontStyle": "italic" }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": { "foreground": "#E5C07B" }
    },
    {
      "scope": [ "entity.other.attribute-name.class", "punctuation.definition.entity" ],
      "settings": { "foreground": "#7CCA8D" }
    },
    {
      "scope": "meta.at-rule.apply.tailwind entity.other.attribute-name.class",
      "settings": { "foreground": "#9FA0FF" }
    },
    {
      "scope": "entity.other.ng-binding-name",
      "settings": { "foreground": "#50AF8E" }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "string.quoted.double punctuation.definition.block",
        "text.html.derivative punctuation.definition.block"
      ],
      "settings": { "foreground": "#99582A" }
    },
    {
      "scope": "support.type.object.module",
      "settings": { "foreground": "#E5C07B" }
    },
    {
      "scope": "support.constant.property-value",
      "settings": { "foreground": "#6A76DF" }
    },
    {
      "scope": [ "support.constant.font-name", "meta.property-value" ],
      "settings": { "foreground": "#E5C07B" }
    },
    { "scope": "punctuation", "settings": { "foreground": "#ABABAB" } },
    {
      "scope": "punctuation.definition.keyword",
      "settings": { "foreground": "#C678DD" }
    },
    {
      "scope": "support.type.vendored",
      "settings": { "foreground": "#7CCA8D" }
    },
    {
      "scope": "constant.character.entity",
      "settings": { "foreground": "#6A76DF" }
    },
    { "scope": [ "source" ], "settings": { "foreground": "#ABABAB" } },
    { "scope": [ "source.ini" ], "settings": { "foreground": "#D19A66" } },
    { "scope": [ "source.python" ], "settings": { "foreground": "#A9DEF9" } },
    {
      "scope": [ "source.python support.type", "support.function.builtin" ],
      "settings": { "foreground": "#2EA7B7", "fontStyle": "italic" }
    },
    {
      "scope": [
        "source.python meta.function",
        "source.python meta.function-call.generic"
      ],
      "settings": { "foreground": "#61AFEF" }
    },
    {
      "scope": [ "meta.function-call" ],
      "settings": { "foreground": "#E5C07B" }
    },
    {
      "scope": [ "markup.heading.markdown", "markup.heading entity.name" ],
      "settings": { "foreground": "#61AFEF" }
    },
    {
      "scope": [ "punctuation.definition.heading.markdown" ],
      "settings": { "foreground": "#455368" }
    }
  ]
}