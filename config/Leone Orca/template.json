    "textLink.foreground": "{{malibu}}",

    "titleBar.activeBackground": "#212530",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#373E49",
    "titleBar.inactiveForeground": "#e7e7e7"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "{{nonPhotoBlue}}"
    },
    "variable.constant": {
      "foreground": "{{whiskey}}"
    },
    "variable.defaultLibrary": {
      "foreground": "{{chalky}}"
    }
  },
  "tokenColors": [
    {
      "scope": ["source.ignore", "keyword.other.definition"],
      "settings": {
        "foreground": "{{nonPhotoBlue}}"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "{{indigo}}"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "{{softPurple}}"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "{{whiskey}}",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "{{fountainBlue}}"
      }
    },
    {
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "{{malibu}}"
      }
    },
    {
      "scope": ["meta.class entity.name.type.class"],
      "settings": {
        "foreground": "{{turquoise}}"
      }
    },
    {
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "{{pelorous}}"
      }
    },
    {
      "scope": [
        "variable.other.readwrite.alias",
        "meta.type.annotation entity.name.type",
        "entity.other.inherited-class",
        "meta.object.member variable.other.readwrite"
      ],
      "settings": {
        "foreground": "{{portage}}"
      }
    },
    {
      "scope": ["constant.language", "support.type.builtin"],
      "settings": {
        "foreground": "{{indigo}}"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "{{turquoise}}"
      }
    },
    {
      "scope": [
        "variable",
        "expression.ng variable.other.readwrite",
        "support.type.property-name",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "{{nonPhotoBlue}}"
      }
    },
    {
      "scope": ["meta.ng-binding.event expression.ng variable.other.readwrite"],
      "settings": {
        "foreground": "{{mint}}"
      }
    },
    {
      "scope": ["variable.other.constant", "support.variable.property"],
      "settings": {
        "foreground": "{{chalky}}"
      }
    },
    {
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "{{iron}}"
      }
    },
    {
      "scope": "variable.language",
      "settings": {
        "foreground": "{{froly}}"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "{{mint}}"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "{{mint}}",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "{{nevada}}"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "{{pelorous}}"
      }
    },
    {
      "scope": ["keyword.operator.new", "keyword.operator.expression"],
      "settings": {
        "foreground": "{{softPurple}}"
      }
    },
    {
      "scope": ["keyword.operator.type", "meta.brace.round", "punctuation.accessor"],
      "settings": {
        "foreground": "{{iron}}"
      }
    },
    {
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "{{pelorous}}"
      }
    },
    {
      "scope": ["meta.brace.round", "punctuation.accessor"],
      "settings": {
        "foreground": "{{silverChalice}}"
      }
    },
    {
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "{{softPurple}}"
      }
    },
    {
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "{{pistachio}}"
      }
    },
    {
      "scope": ["support.class.builtin", "meta.block entity.name.type"],
      "settings": {
        "foreground": "{{froly}}"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "{{froly}}"
      }
    },
    {
      "scope": "entity.name.tag support.class.component",
      "settings": {
        "foreground": "{{mint}}"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "punctuation.definition.ng-binding-name"],
      "settings": {
        "foreground": "{{pistachio}}",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.ng-binding-name",
      "settings": {
        "foreground": "{{mint}}"
      }
    },
    {
      "scope": ["punctuation.definition.string", "string.quoted.double punctuation.definition.block"],
      "settings": {
        "foreground": "{{chalky}}"
      }
    },
    {
      "scope": "support.type.object.module",
      "settings": {
        "foreground": "{{chalky}}"
      }
    },
    {
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "{{indigo}}"
      }
    },
    {
      "scope": "support.constant.font-name",
      "settings": {
        "foreground": "{{chalky}}"
      }
    },
    {
      "scope": "support.type.vendored",
      "settings": {
        "foreground": "{{deYork}}"
      }
    },
    {
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "{{indigo}}"
      }
    },
    {
      "scope": ["source"],
      "settings": {
        "foreground": "{{silverChalice}}"
      }
    }
  ]
}
