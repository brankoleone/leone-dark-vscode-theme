    "peekViewEditor.background": "#1b1d23",
    "peekViewEditor.matchHighlightBackground": "#29244b",
    "peekViewResult.background": "#20252E",
    "scrollbarSlider.activeBackground": "#747d9180",
    "scrollbarSlider.background": "#4e566660",
    "scrollbarSlider.hoverBackground": "#5a637580",

    "gitDecoration.ignoredResourceForeground": "#343e50",
    "gitDecoration.modifiedResourceForeground": "{{malibu}}",
    "gitDecoration.deletedResourceForeground": "#ff616e",
    "gitDecoration.untrackedResourceForeground": "{{deYork}}",
    "editorGutter.addedBackground": "{{mint}}",
